      },
      "name": "maxTravelSpeed",
      "schema": "double",
      "unit": "metrePerSecond",
      "writable": true
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "maximum travel speed unit"
