{
  "@id": "dtmi:com:syyclops:ElectricalCircuit;1",
  "@type": "Interface",
  "displayName": {
    "en": "Electrical Circuit"
  },
  "extends": [
    "dtmi:com:syyclops:ElectricalSystem;1"
  ],
  "contents": [
    {
      "@type": [
        "Property",
        "Voltage"
      ],
      "name": "voltage",
      "displayName": {
        "en": "Voltage"
      },
      "writable": true,
      "schema": "integer",
      "unit": "volt",
      "@id": "dtmi:com:syyclops:ElectricalCircuit:voltage;1"
    },
    {
      "@type": "Property",
      "name": "poles",
      "displayName": {
        "en": "Poles"
      },
      "writable": true,
      "schema": {
        "@type": "Enum",
        "valueSchema": "string",
        "enumValues": [
          {
            "name": "One",
            "displayName": "1",
            "enumValue": "1",
            "@id": "dtmi:com:syyclops:ElectricalCircuit:One;1"
          },
          {
            "name": "Two",
            "displayName": "2",
            "enumValue": "2",
            "@id": "dtmi:com:syyclops:ElectricalCircuit:Two;1"
          },
          {
            "name": "Three",
            "displayName": "3",
            "enumValue": "3",
            "@id": "dtmi:com:syyclops:ElectricalCircuit:Three;1"
          }
        ],
        "@id": "dtmi:com:syyclops:ElectricalCircuit:ES1;1"
      },
      "@id": "dtmi:com:syyclops:ElectricalCircuit:poles;1"
    },
    {
      "@type": [
        "Property",
        "Current"
      ],
      "name": "CBRating",
      "displayName": {
        "en": "Circuit Breaker Rating"
      },
      "writable": true,
      "schema": "integer",
      "unit": "ampere",
      "@id": "dtmi:com:syyclops:ElectricalCircuit:CBRating;1"
    },
    {
      "@type": "Property",
      "name": "wireSize",
      "displayName": {
        "en": "Wire Size"
      },
      "writable": true,
      "schema": "string",
      "comment": "Qty-Size Phase, Qty-Size Neutral, Qty-Size Ground",
      "@id": "dtmi:com:syyclops:ElectricalCircuit:wireSize;1"
    },
    {
      "@type": "Property",
      "name": "wireType",
      "displayName": {
        "en": "Wire Type"
      },
      "writable": true,
      "schema": "string",
      "@id": "dtmi:com:syyclops:ElectricalCircuit:wireType;1"
    },
    {
      "@type": "Property",
      "name": "loadClassification",
      "displayName": {
        "en": "Load Classification"
      },
      "schema": {
        "@type": "Map",
        "mapKey": {
          "name": "loadClassification",
          "description": {
            "en": "A shortened DTDL Model Identifier (doesn't include the full path or version)  of an asset or group of assets that are connected to this circuit (i.e. HVACEquipment, AirHandlingUnit)."
          },
          "schema": "string",
          "@id": "dtmi:com:syyclops:ElectricalCircuit:MM3;1"
        },
        "mapValue": {
          "name": "loadParameters",
          "schema": {
            "@type": "Object",
            "fields": [
              {
                "name": "consumptionPercent",
                "description": {
                  "en": "The percent of the total energy consumed by this circuit that this load classification accounts for."
                },
                "schema": "double",
                "@id": "dtmi:com:syyclops:ElectricalCircuit:consumptionPercent;1"
              },
              {
                "name": "connectedLoad",
                "description": {
                  "en": "The total apparent power in VA connected to this circuit that this load classification accounts for."
                },
                "schema": "double",
                "@id": "dtmi:com:syyclops:ElectricalCircuit:connectedLoad;1"
              },
              {
                "name": "demandFactor",
                "description": {
                  "en": "The percent of the connected load that is consuming energy at any given time. This can be used to calculate the demand load as a percentage of the connected load."
                },
                "schema": "double",
                "@id": "dtmi:com:syyclops:ElectricalCircuit:demandFactor;1"
              }
            ],
            "@id": "dtmi:com:syyclops:ElectricalCircuit:OS4;1"
          },
          "@id": "dtmi:com:syyclops:ElectricalCircuit:loadParameters;1"
        },
        "@id": "dtmi:com:syyclops:ElectricalCircuit:MS2;1"
      },
      "@id": "dtmi:com:syyclops:ElectricalCircuit:loadClassification;1"
    }
