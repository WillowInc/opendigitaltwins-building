{
  "@id": "dtmi:com:willowinc:Boiler;1",
  "@type": "Interface",
  "displayName": {
    "en": "Boiler"
  },
  "extends" : [
    "dtmi:com:willowinc:PlumbingEquipment;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "displayName": {
        "en": "type"
      },
      "name": "type",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "WaterTube",
            "name": "WaterTube"
          },
          {
            "enumValue": "FireTube",
            "name": "FireTube"
          }
        ],
        "valueSchema": "string"
      },
      "writable": true
    },
    {
      "@type": ["Property", "Volume"],
      "displayName": {
        "en": "tank capacity"
      },
      "name": "tankCapacity",
      "schema": "double",
      "unit": "litre",
      "writable": true
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "tank capacity unit"
      },
      "name": "tankCapacityUnit",
      "annotates": "tankCapacity",
      "overrides": "unit",
      "schema": "VolumeUnit",
      "writable": true
    },
    {
      "@type": ["Power", "Property"],
      "displayName": {
        "en": "power output"
      },
      "name": "powerOutput",
      "schema": "double",
      "unit": "watt",
      "writable": true
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "power output unit"
      },
      "name": "powerOutputUnit",
      "annotates": "powerOutput",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true
    },
    {
      "@type": ["Power", "Property"],
      "displayName": {
        "en": "power input"
      },
      "name": "powerInput",
      "schema": "double",
      "unit": "watt",
      "writable": true
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "power input unit"
      },
      "name": "powerInputUnit",
      "annotates": "powerInput",
      "overrides": "unit",
      "schema": "PowerUnit",
      "writable": true
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "medium"
      },
      "name": "medium",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Steam",
            "name": "Steam"
          },
          {
            "enumValue": "HotWater",
            "name": "HotWater"
          }
        ],
        "valueSchema": "string"
      },
      "writable": true
    },
    {
      "@type": ["Property", "VolumeFlowRate"],
      "description": {
        "en": "Imperial measure for the recovery rate of a boiler (gallons per hour)."
      },
      "displayName": {
        "en": "recovery @ 100F rise"
      },
      "name": "recovery100FRise",
      "schema": "integer",
      "unit": "gallonPerHour",
      "writable": true
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "designEnteringWaterTemperature",
      "displayName": {
        "en": "Design Entering Water Temperature"
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Design Entering Water Temperature Unit"
      },
      "name": "designEnteringWaterTemperatureUnit",
      "annotates": "designEnteringWaterTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "designLeavingWaterTemperature",
      "displayName": {
        "en": "Design Leaving Water Temperature"
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Design Leaving Water Temperature Unit"
      },
      "name": "designLeavingWaterTemperatureUnit",
      "annotates": "designLeavingWaterTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "designDeltaWaterTemperature",
      "displayName": {
        "en": "Design Delta Water Temperature"
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Design Delta Water Temperature Unit"
      },
      "name": "designDeltaWaterTemperatureUnit",
      "annotates": "designDeltaWaterTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": "Property",
      "name": "thermalEfficiency",
      "displayName": {
        "en": "Thermal Efficiency"
      },
      "description": {
        "en": "The ratio of useful heat output to the total energy input, expressed as a percentage."
      },
      "schema": "double",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "turndownRatio",
      "displayName": {
        "en": "Turndown Ratio"
      },
      "description": {
        "en": "The ratio of the maximum to minimum output power of the boiler."
      },
      "schema": "double",
      "writable": true
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "maxLeavingWaterTemperature",
      "displayName": {
        "en": "Maximum Leaving Water Temperature"
      },
      "description": {
        "en": "The maximum water temperature leaving the boiler used when the outdoor temperature is at or below a specified low limit."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Maximum Leaving Water Temperature Unit"
      },
      "name": "maxLeavingWaterTemperatureUnit",
      "annotates": "maxLeavingWaterTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
        {
      "@type": [ "Property", "Temperature" ],
      "name": "minLeavingWaterTemperature",
      "displayName": {
        "en": "Minimum Leaving Water Temperature"
      },
      "description": {
        "en": "The minimum water temperature leaving the boiler used when the outdoor temperature is at a specified high limit."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Minimum Leaving Water Temperature Unit"
      },
      "name": "minLeavingWaterTemperatureUnit",
      "annotates": "minLeavingWaterTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "outdoorTemperatureLowLimit",
      "displayName": {
        "en": "Outdoor Temperature Low Limit"
      },
      "description": {
        "en": "The outdoor temperature below which the maximum leaving water temperature is used."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Outdoor Temperature Low Limit Unit"
      },
      "name": "outdoorTemperatureLowLimitUnit",
      "annotates": "outdoorTemperatureLowLimit",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": [ "Property", "Temperature" ],
      "name": "outdoorTemperatureHighLimit",
      "displayName": {
        "en": "Outdoor Temperature High Limit"
      },
      "description": {
        "en": "The outdoor temperature at which the minimum leaving water temperature is used. Outdoor temperatures above this limit typically result in the boiler being off."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Outdoor Temperature High Limit Unit"
      },
      "name": "outdoorTemperatureHighLimitUnit",
      "annotates": "outdoorTemperatureHighLimit",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
        {
      "@type": [ "Property", "Temperature" ],
      "name": "lockoutOutdoorTemperature",
      "displayName": {
        "en": "Lockout Outdoor Temperature"
      },
      "description": {
        "en": "The outdoor temperature above which the boiler is locked out and does not operate preventing unneccesary heating."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Lockout Outdoor Temperature Unit"
      },
      "name": "lockoutOutdoorTemperatureUnit",
      "annotates": "lockoutOutdoorTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ]
}
