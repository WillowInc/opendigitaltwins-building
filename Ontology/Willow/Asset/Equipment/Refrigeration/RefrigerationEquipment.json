{
  "@id": "dtmi:com:willowinc:RefrigerationEquipment;1",
  "@type": "Interface",
  "displayName": {
    "en": "Refrigeration Equipment"
  },
  "description": {
    "en": "An asset that is designed to contain and use refrigerant gas to establish or maintain colder than ambient temperatures in a confined space."
  },
  "extends" : [
    "dtmi:com:willowinc:Equipment;1"
  ],
  "contents": [
    {
      "@type": "Property",
      "displayName": {
        "en": "Defrost Type"
      },
      "name": "defrostType",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Off Cycle",
            "name": "OffCycle"
          },
          {
            "enumValue": "Electric",
            "name": "Electric"
          },
          {
            "enumValue": "Hot Gas",
            "name": "HotGas"
          },
          {
            "enumValue": "Reverse Air",
            "name": "ReverseAir"
          }
        ],
        "valueSchema": "string"
      },
      "writable": true
    },{
      "@type": [ "Property", "Temperature" ],
      "name": "defrostTerminationTemperature",
      "displayName": {
        "en": "Defrost Termination Temperature"
      },
      "description": {
        "en": "The temperature setpoint at which a defrost cycle is set to stop running, typically on an evaporator coil."
      },
      "schema": "double",
      "unit": "degreeCelsius"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Defrost Termination Temperature Unit"
      },
      "name": "defrostTerminationTemperatureUnit",
      "annotates": "defrostTerminationTemperature",
      "overrides": "unit",
      "schema": "TemperatureUnit"
    },
    {
      "@type": [ "Property", "TimeSpan" ],
      "name": "defrostStartInterval",
      "displayName": {
        "en": "Defrost Start Interval"
      },
      "description": {
        "en": "The time interval between the start of each defrost cycle, typically defined in hours, when the controls are configured to start defrost based on a set time interval."
      },
      "schema": "double",
      "unit": "hour"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Defrost Start Interval Unit"
      },
      "name": "defrostStartIntervalUnit",
      "annotates": "defrostStartInterval",
      "overrides": "unit",
      "schema": "TimeUnit"
    },
    {
      "@type": [ "Property", "TimeSpan" ],
      "name": "defrostMaxDuration",
      "displayName": {
        "en": "Defrost Max Duration"
      },
      "description": {
        "en": "The duration, typically defined in minutes, from the start to the end of a defrost cycle."
      },
      "schema": "double",
      "unit": "minute"
    },
    {
      "@type": ["Property", "ValueAnnotation", "Override"],
      "displayName": {
        "en": "Defrost Max Duration Unit"
      },
      "name": "defrostMaxDurationUnit",
      "annotates": "defrostMaxDuration",
      "overrides": "unit",
      "schema": "TimeUnit"
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Defrost Start Method"
      },
      "name": "defrostStartMethod",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Interval Timer",
            "name": "IntervalTimer"
          },
          {
            "enumValue": "Schedule",
            "name": "Schedule"
          },
          {
            "enumValue": "Adaptive",
            "name": "Adaptive"
          }
        ],
        "valueSchema": "string"
      },
      "writable": true
    },
    {
      "@type": "Property",
      "displayName": {
        "en": "Defrost Stop Method"
      },
      "name": "defrostStopMethod",
      "schema": {
        "@type": "Enum",
        "enumValues": [
          {
            "enumValue": "Time",
            "name": "Time"
          },
          {
            "enumValue": "Termination Temperature",
            "name": "TerminationTemperature"
          },
          {
            "enumValue": "Adaptive",
            "name": "Adaptive"
          }
        ],
        "valueSchema": "string"
      },
      "writable": true
    }
  ],
  "@context": [
    "dtmi:dtdl:context;3",
    "dtmi:dtdl:extension:quantitativeTypes;1",
    "dtmi:dtdl:extension:annotation;1",
    "dtmi:dtdl:extension:overriding;1"
  ]
}
